{
  "name": "@ohfinance/oh-app",
  "description": "Oh! Finance App",
  "version": "0.2.1",
  "license": "MIT",
  "author": "Oh! Finance <hello@oh.finance>",
  "private": true,
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  },
  "volta": {
    "node": "14.17.6",
    "yarn": "1.22.11"
  },
  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build && node ./scripts/generate-sitemap",
    "export": "npm run build && next export -o _static",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir src --dir scripts",
    "format": "npx prettier src scripts ./*.js ./*.json --check",
    "format:fix": "npx prettier src scripts ./*.js ./*.json --write",
    "prepare": "npx husky install"
  },
  "dependencies": {
    "@fontsource/open-sans": "^4.5.1",
    "@popperjs/core": "^2.10.2",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "^6.2.4",
    "add": "^2.0.6",
    "autoprefixer": "^10.3.7",
    "axios": "^0.23.0",
    "bignumber.js": "^9.0.1",
    "d3-dsv": "^3.0.1",
    "d3-time-format": "^4.0.0",
    "ethers": "^5.4.7",
    "md5": "^2.3.0",
    "next": "11.1.2",
    "next-compose-plugins": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "recharts": "^2.1.5",
    "tailwindcss": "^2.2.17",
    "typescript": "^4.4.4",
    "web3": "^1.6.0",
    "web3-core": "^1.6.0",
    "web3-eth-contract": "^1.6.0",
    "web3-utils": "^1.6.0",
    "zustand": "^3.5.13"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^11.1.2",
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/typography": "^0.4.1",
    "@types/d3-dsv": "^3.0.0",
    "@types/d3-time-format": "^4.0.0",
    "@types/md5": "^2.3.1",
    "@types/react": "17.0.30",
    "@types/tailwindcss": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "7.32.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "trash-cli": "4.0.0"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
